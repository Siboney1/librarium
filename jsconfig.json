{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "shared/*": [
        "src/shared/*"
      ],
      "utils/*": [
        "src/shared/utils/*"
      ],
      "content/*": [
        "content/*"
      ],
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*"
  ]
}