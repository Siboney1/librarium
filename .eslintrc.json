{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "import",
    "jsx-a11y",
    "prettier"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "prettier/prettier": 1,
    "react/prop-types": 0,
    "prefer-destructuring": 0,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "no-async-promise-executor": 0,
    "no-unused-vars": 1,
    "no-console": [
      1,
      {
        "allow": [
          "warn",
          "error",
          "trace"
        ]
      }
    ],
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "graphql": false
  }
}